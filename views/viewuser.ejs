      <section class="section">
        <div class="row">
          <div class="col-lg-12">
            <div class="card" style="overflow-x: scroll">
              <div class="card-body">
                <h5 class="card-title">Datatables</h5>
                <!-- Table with stripped rows -->
                <table class="table datatable">
                  <thead>
                    <tr>
                      <th scope="col">S No.</th>
                      <th scope="col">Name</th>
                      <th scope="col">Email</th>
                      <th scope="col">Mobile</th>
                      <th scope="col">UserId</th>
                      <th scope="col">ParentId</th>
                      
                    </tr>
                  </thead>

                  <tbody>
                    <div>
                      <!-- <%let i=1; %> <% for (let dataobj of usersdata){ %> -->
                      <tr>
                        <th scope="row"><%=i%></th>
                        <td><%=dataobj.name%></td>
                        <td><%=dataobj.email%></td>
                        <td><%=dataobj.mobile%></td>
                        <td><%=dataobj.userId%></td>
                        <td><%=dataobj.parentId%></td>
                        
                        
                        <%id=dataobj._id.toString();%>
                        <td>
                          <form
                            class="filter"
                            action="/user_block"
                            method="post"
                          >
                            <input type="hidden" name="id" value="<%=id%>" />
                            <input
                              type="hidden"
                              name="access"
                              value="<%=dataobj.access%>"
                            />
                            <%if(dataobj.access){%>

                            <input
                              type="submit"
                              class="btn btn-success"
                              value="unblock"
                            />
                            <%}else{%>
                            <input
                              type="submit"
                              class="btn btn-danger"
                              value="block"
                            />
                            <%}%>
                          </form>
                        </td>
                        <td>
                          <form
                            class="filter"
                            action="/user_mail_page"
                            method="post"
                          >
                            <input
                              type="hidden"
                              name="email"
                              value="<%=dataobj.email%>"
                            />
                            <input
                              type="submit"
                              value="Mail"
                              class="btn btn-primary"
                            />
                          </form>
                        </td>
                        <td>
                          <form
                            class="filter"
                            action="/user_update_page"
                            method="post"
                          >
                            <input type="hidden" name="id" value="<%=id%>" />
                            <input
                              class="flter_input btn btn-info"
                              type="submit"
                              value="Update Profile"
                            />
                          </form>
                        </td>
                        <%userid=dataobj._id.toString();%>
                        <td>
                          <form
                            class="filter"
                            action="/update_points"
                            method="post"
                          >
                            <input
                              type="hidden"
                              name="id"
                              value="<%=userid%>"
                            />

                            <input
                              type="submit"
                              class="btn btn-warning"
                              value="Update Amount"
                            />
                          </form>
                        </td>
                        <td>
                          <form
                            class="filter"
                            action="/delete_user"
                            method="post"
                            onsubmit="return confirmDelete();"
                          >
                            <input
                              type="hidden"
                              name="id"
                              value="<%=userid%>"
                            />

                            <input
                              type="submit"
                              class="btn btn-dark"
                              value="Delete User"
                            />
                          </form>
                        </td>

                        <td>
                          <form
                            class="filter"
                            action="/view_user_commodity"
                            method="get"
                          >
                            <input
                              type="hidden"
                              name="id"
                              value="<%=userid%>"
                            />

                            <input
                              type="submit"
                              class="btn btn-info"
                              value="View Profile"
                            />
                          </form>
                        </td>
                      </tr>
                      <% i=i+1; }%>
                    </div>
                  </tbody>
                </table>
                <!-- End Table with stripped rows -->
              </div>
            </div>
          </div>
        </div>
       
      </section>
    </main>
    <!-- End #main -->

    <%- include("../utils/footer")%>

    <a
      href="#"
      class="back-to-top d-flex align-items-center justify-content-center"
      ><i class="bi bi-arrow-up-short"></i
    ></a>
    <script>
      function confirmDelete() {
        // Display a confirmation dialog
        var result = confirm("Are you sure you want to delete this user?");

        // If the user clicks OK, the form will be submitted. Otherwise, it won't.
        return result;
      }
    </script>
